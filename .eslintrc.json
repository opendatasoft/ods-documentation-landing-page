{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "plugins": [
    "html"
  ],
  "globals": {
    "hbspt": false,
    "Promise" : true
  }
}
